WARNS="-Wconversion -Wunreachable-code"
WARNS="-Wall -Wextra -Werror -Wshadow -pedantic-errors   -Wimplicit -Wimplicit-function-declaration -Wimplicit-int -Wmissing-declarations -Wmissing-prototypes -Wold-style-definition -Wstrict-prototypes -Wbad-function-cast -Wcast-qual -Wcast-align -Wwrite-strings -Waggregate-return -Wredundant-decls -Wnested-externs -Wuninitialized -O"
#c99 $WARNS -D_GNU_SOURCE -Wno-unused -g -rdynamic iio.c piio.c -ljpeg -ltiff -lpng
#c99 $WARNS -D_GNU_SOURCE -Wno-unused -g -rdynamic -c iio.c
export CPATH=/usr/include/OpenEXR
EXRLIB="-lIex -lHalf -lIlmImf"
c99 $WARNS -DI_CAN_HAS_LINUX -DNDEBUG -DIIO_SHOW_DEBUG_MESSAGESx -D_GNU_SOURCE -Wno-unused -Wno-cast-qual -g  -c iio.c
c99 $WARNS -DNDEBUG -DI_CAN_HAS_LINUX -D_GNU_SOURCE -Wno-unused -O3 -c iio.c
c99 -Wno-unused -g iio_test.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_test
c99 -Wno-unused -g iio_utest.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_utest
c99 -Wno-unused -g iio_test_named.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_test_named
c99 -Wno-unused -g iio_test_nameds.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_test_nameds
c99 -Wno-unused -g iio_split.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_split
c99 -Wno-unused -g iio_unsplit.c iio.o -ljpeg -ltiff -lpng $EXRLIB -o iio_unsplit
cp iio_test ~/bin/iio
cp iio_utest ~/bin/iiou
cp iio_test_named ~/bin/iion
#c99 -Wno-unused -g simplecube.c iio.o -ljpeg -ltiff -lpng -o simplecube
#cc99 -c -g zoombil.c  && ^C99 -g zoombil.o iio.o -o zoombil -ltiff -ljpeg -lpng -lIex -lHalf -lIlmImf
